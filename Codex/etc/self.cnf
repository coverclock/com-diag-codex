[ req ]
prompt                 = no
output_password        = s31f
default_bits           = 3072
default_md             = sha256
distinguished_name     = req_distinguished_name

[ req_distinguished_name ]
C                      = US
ST                     = Colorado
L                      = Fort Collins
O                      = Prairie Thorn
OU                     = Hacker
CN                     = hacker.prairiethorn.org
emailAddress           = hacker@prairiethorn.org

[ usr_cert ]
basicConstraints       = CA:FALSE
nsComment              = "OpenSSL Generated Certificate"
subjectKeyIdentifier   = hash
authorityKeyIdentifier = keyid,issuer
subjectAltName         = DNS:hacker.prairiethorn.org
